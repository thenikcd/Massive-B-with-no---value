#include <conio.h>
#include <stdio.h> 
#include <math.h> 
int main() { 
	int A[100], B[100], i, j, n; 
	printf("Input n:"); 
	scanf("%d", &n); 
	printf("Array:\n"); 
	for(i=0;i<n;i++) 
		scanf("%d", A[i]); 
	for(i=0;i<n;i++) 
		for(j=0;j<n;j++) 
		{ 
			if (A[i]<0) 
				A[i]=B[j]; 
		} 
	for(i=0;i<n;i++) 
		printf(" %d ", B[i]); 
	getch(); 
	return 0; }
